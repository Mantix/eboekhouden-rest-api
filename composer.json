{
    "name": "mantix/eboekhouden-rest-api",
    "description": "PHP client library for the new e-Boekhouden REST API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mantix B.V.",
            "email": "support@mantix.nl"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Mantix\\EBoekhoudenRestApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mantix\\EBoekhoudenRestApi\\Tests\\": "tests/"
        }
    }
}